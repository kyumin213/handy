      </view> -->
      <view class='fzCol6 footprint'>汗的足迹</view>
      <view class='showPrint'>
        <view class='footItem'>
          <view class='Num'>{{personAll.entranceCount}}</view>
          <view class='fzCol6 txt'>上课/次</view>
        </view>
        <view class='footItem'>
          <view class='Num'>{{personAll.memberBalanceMoney}}</view>
          <view class='fzCol6 txt'>余额</view>
        </view>
        <view class='footItem'>
          <view class='Num'>{{personAll.privateSum}}</view>
          <view class='fzCol6 txt'>私教/节</view>
        </view>
      </view>
    </view>
    <view class='card' bindtap='myHandyCard'>
      <text class='fzCol6 memberCard'>汗滴卡</text>
      <view class='openUp'>
        <text>详情</text>
        <text class='jt'></text>
      </view>
    </view>

  </view>
  <view class='myAppoitment'>
    <view class='fzCol6 yuTitle'>我的预约</view>
    <view class='fzCol6 yuList'>
      <view class='yuItems' bindtap='djxShow'>
        <view class='itemImg'>
          <image class='listIcon' mode='widthFix' src='../../images/me/Waitfor@2x.png'></image>
        </view>
        <view class='itemTxt'>待进行</view>
      </view>
      <view class='yuItems' bindtap='jxzShow'>
        <view class='itemImg'>
          <image class='listIcon' mode='widthFix' src='../../images/me/Theallelic@2x.png'></image>
        </view>
        <view class='itemTxt'>进行中</view>
      </view>
      <view class='yuItems' bindtap='ywcShow'>
        <view class='itemImg'>
          <image class='listIcon' mode='widthFix' src='../../images/me/Completed@2x.png'></image>
        </view>
        <view class='itemTxt'>已完成</view>
      </view>
