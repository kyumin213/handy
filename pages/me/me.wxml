<!--pages/me/me.wxml-->
<view>

  <view class='headBg'>
    <image class='headBgImg' mode='widthFix' src='../../images/me/bg.png'></image>
  </view>

  <view class='headCon'>
    <view class='headTop'>
      <view class='headImgBox'>
        <open-data type="userAvatarUrl" class="userinfo-avater" mode="cover"></open-data>
        <!-- <image class='headImg' src="{{userInfo.avatarUrl}}" mode='widthFix'></image> -->
      </view>
      <view class='fzCol6 userName'>
        <open-data type="userNickName" lang="zh_CN"></open-data>
      </view>
      <!-- <view class='fzCol6 viewCode' bindtap='viewQRcode'>查看二维码</view> -->
      <!-- <view>
